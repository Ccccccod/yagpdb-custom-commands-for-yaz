{{else if in (cslice "poll" "polls") $command}}
{{$syntax := "**Syntax**: `=activity poll Write your poll here`\nThat's enough to make a normal poll\n```Advanced```But if you want to make a multiple choice poll or change embed color:\n`=activity poll question | option 1 / option 2 / ... | color (optional)`\nWhere:\n`question` is your question.\n`option 1, 2...` is options people can vote. Separate options with `/`. You can add up to 20 options.\n`color` is decimal color code to change embed color, must be between 0 - 16777215.\n**Notes:** you need `|` to separate title, content,... and `/` to separate options.\nFor Examples: =polls Do you want to eat fish? | 16776960\n=polls Do you want to drink milk, coffee or wine | ü•õ / ‚òï / üç∑"}}
{{$syntax := "**Syntax**: `=activity poll Do you like strawberries?` The bot will react with ‚úÖ and ‚ùå.\n\nIf you want a **multiple choice** poll:\n`=activity poll question | Option 1 / Option 2 / Option 3 | Color`\n You can have up to **20** options\n\n **An Example**: `=activity poll Which game is your favorite? | Rocket League / Diep / MineCraft / Terraria` The bot will react with the numbers next to each option, and the option's name.\n **Note**: You can do `=activity poll <question> | test` to see how this will look in <#575494044864479262> (Coming soon)\n **For Example**: `=activity poll Do you like strawberries? | #000001`\n"}}
{{$args := parseArgs 2 $syntax (carg "string" "poll") (carg "string" "content")}}
{{$polls := 575494044864479262}}
{{$description := $args.Get 1}}{{$args = (split $description "|")}}
{{$att := ""}}{{if gt (len .Message.Attachments) 0}}{{$att = (index .Message.Attachments 0).URL}}{{end}}
{{$how := "https://discordapp.com/channels/529138809246580745/575494044864479262/649633139404308482"}}
{{$author := (sdict "name" "Author" "value" (joinStr "" .User.Mention "\nHow to make a poll: [Click here](" $how ")"))}}
{{if gt (len $args) 2}}{{with lower (reFind `^[0-9a-fA-F]{1,6}$` (index $args 2))}}
	{{$h := .}}{{$z := 0}}{{$e := 0}}
	{{$lenHex := len . | mult -1 | add 1}}
	{{range $k, $kk := seq $lenHex 1}}
		{{$v := index $h $k}}
		{{$a := ""}}
		{{if and (ge $v 48) (le $v 57)}}
		{{$a = printf "%c" $v}}
		{{else if and (ge $v 97) (le $v 102)}}{{$a = add $v -87}}{{end}}
		{{$kk = mult $kk -1}}
		{{$z = add $z (mult $a (pow 16 $kk))}}
	{{end}}
	{{$col = $z}}
{{end}}{{end}}
{{$fields := cslice}}
{{$description = reReplace `\n{2,}| +` (index $args 0) " "}}
{{$emojis := cslice}}
{{range (getMessage 631814483181830144 657309180717170708).Reactions}}
	{{$emojis = $emojis.Append .Emoji}}
{{end}}
{{$mult := false}}
{{if gt (len $args) 1}}{{if ne (reFind `\/` (index $args 1)) ""}}
